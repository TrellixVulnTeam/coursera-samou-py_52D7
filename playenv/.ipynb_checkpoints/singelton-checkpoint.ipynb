    "class Singelton:\n",
    "    isinstance = None\n",
    "\n",
    "    def __new__(cls):\n",
    "        if cls.isinstance is None:\n",
    "            cls.isinstance = super().__new__(cls)\n",
    "            \n",
    "        return cls.isinstance\n",
    "\n",
    "a = Singelton()\n",
    "b = Singelton()\n",
    "a is b"
